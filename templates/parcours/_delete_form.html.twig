<form method="post" action="{{ path('app_parcours_delete', {'id': parcour.id}) }}"
      onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ parcour.id) }}">
    <button class="btn btn-sm btn-danger">
        <i class="fa fa-trash"></i>
        Supprimer
    </button>
</form>
